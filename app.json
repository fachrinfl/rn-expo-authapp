{
  "expo": {
    "name": "AuthApp",
    "slug": "AuthApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fachrinfl.AuthApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": ["assets/fonts/*"],
    "plugins": ["expo-font"],
    "extra": {
      "eas": {
        "projectId": "20494492-1b96-4080-9788-61aad5feabb7"
      }
    }
  }
}
